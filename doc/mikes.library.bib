@Article{	  shallice84*1,
  author	= {Shallice, Tim},
  title		= {Psychology and social control},
  journal	= {Cognition},
  volume	= {17},
  number	= {1},
  pages		= {29-48},
  abstract	= {Assesses the history of social control applications of
		  psychology and the likelihood of a future increase in their
		  importance, focusing on the effects of military funding of
		  psychological research and the social consequences of
		  widespread unemployment. It is argued that psychology and
		  related areas of cognitive science and neuroscience may
		  become increasingly relevant in the development of the
		  technical components of such techniques rather than in
		  providing ideological justifications for their use. George
		  Orwell's 1984 vision that psychological techniques will be
		  used to impose acceptance of a political system on a
		  population who might otherwise reject it is discussed, and
		  a lack of discussion by psychologists on the subject is
		  noted. It is suggested that this lack is due to a framework
		  of social science research that tends to become reduced to
		  narrow questions of the ethics of the investigation itself
		  rather than to the consequences it might have. (French
		  abstract)},
  year		= {1984}
}

@Article{	  shallice93,
