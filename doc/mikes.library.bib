@Article{	  kawamoto.anderson85,
  author	= {Kawamoto, Alan H. and Anderson, James A.},
  title		= {A neural network model of multistable perception.
		  {S}pecial Issue: {S}eeing and knowing},
  journal	= {Acta Psychologica},
  volume	= {59},
  number	= {1},
  pages		= {35-65},
  abstract	= {Reviews major properties and models of multistable
		  perception and proposes a neural network model based on
		  state vectors, feedback, and Hebbian synaptic modification.
		  Multistable perception refers to visual stimuli that can be
		  perceived in 1 of 2 possible configurations, with a
		  perceived organization that can change spontaneously from
		  one configuration to another. Evidence demonstrating the
		  importance of both the stimulus and the recent and distant
		  history of the system to disambiguate ambiguous stimuli is
		  presented.},
  year		= {1985}
}

@Article{	  kawamoto.farrar.ea94,
  author	= {Kawamoto, Alan H. and Farrar, William T. and Kello,
		  Christopher T.},
  title		= {When two meanings are better than one: {M}odeling the
		  ambiguity advantage using a recurrent distributed network.
		  Special Section: Modeling visual word recognition},
  journal	= jephpp,
  volume	= {20},
  number	= {6},
  pages		= {1233-1247},
  abstract	= {Ambiguous words are processed more quickly than
		  unambiguous words in a lexical decision task despite the
		  fact that each sense of an ambiguous word is less frequent
		  than the single sense of unambiguous words of equal
		  frequency or familiarity. In this computer simulation
		  study, we examined the effects of different assumptions of
		  a fully recurrent connectionist model in accounting for
		  this processing advantage for ambiguous words. We argue
		  that the ambiguity advantage effect can be accounted for by
		  distributed models if (a) the least mean square (LMS)
		  error-correction algorithm rather than the Hebbian
		  algorithm is used in training the network and (b)
		  activation of the units representing the spelling rather
		  than the meaning is used to index word recognition times.},
  year		= {1994}
}

@Article{	  kawamoto.zemblidge92,
  author	= {Kawamoto, Alan H. and Zemblidge, John H.},
  title		= {Pronunciation of homographs},
  journal	= jml,
  volume	= {31},
  number	= {3},
  pages		= {349-374},
  abstract	= {Conducted 4 experiments with 195 undergraduates to test
		  the predictions concerning the pronunciation of
		  monosyllabic homographs made by the dual-route model, the
		  analogy model, and the distributed model. Naming latency
		  for monosyllabic homographs was longer than for regular
		  control words that were half the printed frequency of the
		  homographs. The more frequent pronunciation was the
		  irregular pronunciation for the majority of the
		  monosyllabic homographs, and the naming latency for the
		  more frequent pronunciation of these homographs was the
		  same as or longer than the naming latency for the less
		  frequent pronunciation. The naming latency difference
		  between homographs and their controls was larger than that
		  between exceptions and their controls. The proportion of a
		  given pronunciation was highly correlated with the
		  subjective familiarity of that pronunciation.},
  year		= {1992}
}

