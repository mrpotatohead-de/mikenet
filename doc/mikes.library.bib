@Article{	  patterson.besner84*1,
  author	= {Patterson, Karalyn and Besner, Derek},
  title		= {Is the right hemisphere literate?},
  journal	= {Cognitive Neuropsychology},
  volume	= {1},
  number	= {4},
  pages		= {315-341},
  abstract	= {M. Coltheart (1980, 1983) proposed that in the typical
		  right- handed, left-hemisphere-dominant individual, the
		  right hemisphere is literate, with limitations. The primary
		  skill of this right-hemisphere reading system is thought to
		  be comprehension of printed words, especially
		  concrete/imageable words; its failings are thought to
		  include the capacities to convert an orthographic code to a
		  phonological one, to deal with abstract words, and to
		  distinguish among semantically related words. Coltheart
		  argued that the reading performance of deep dyslexic
		  patients derives from the right-hemisphere reading system.
		  His evidence for this proposal involves the claim that, in
		  reading-related tasks with orthographic input lateralized
		  to the left visual field, both split-brain patients and
		  normal Ss behave similarly to deep dyslexics. A review of
		  this evidence does not support the existence of a
		  right-hemisphere reading system with a predictable level of
		  skill and pattern of characteristics.},
  year		= {1984}
}

