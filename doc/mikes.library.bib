@Article{	  coltheart.al83,
  author	= {Coltheart, Max and et al.},
  title		= {Surface dyslexia},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {35},
  number	= {3-A},
  pages		= {469-495},
  abstract	= {Describes 2 cases of surface dyslexia. One of the cases
		  was a developmental dyslexic, and the other was an acquired
		  dyslexic. The close similarity of their reading and
		  spelling performance supports the view that surface
		  dyslexia can occur both as a developmental and as an
		  acquired dyslexia. It is postulated that defects within the
		  input logogen system, and in communication from that system
		  to semantics, are responsible for most of the symptoms of
		  surface dyslexia.},
  year		= {1983}
}

@Article{	  coltheart.avons.ea90,
  author	= {Coltheart, Veronika and Avons, S. E. and Trollope, Julie},
  title		= {Articulatory suppression and phonological codes in reading
		  for meaning},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {42},
  number	= {2-A},
  pages		= {375-399},
  abstract	= {Examined whether the phonological code that causes errors
		  in printed sentence comprehension is affected by concurrent
		  articulation. 40 college students made speeded judgments of
		  the acceptability of printed sentences that were (1)
		  orthographically unacceptable but phonologically acceptable
		  and (2) orthographically and phonologically unacceptable.
		  20 Ss performed this task in silence and showed a marked
		  phonological effect. The remaining Ss, who performed this
		  task with concurrent articulatory suppression (SUP), showed
		  an increase in false alarm rates but no effect of
		  phonology. In a control experiment using the same Ss, the
		  word length effect disappeared under SUP, indicating that
		  the SUP manipulation was highly effective. Thus, the
		  phonological codes that are used in sentence comprehension
		  and memory span are highly susceptible to articulatory
		  SUP.},
  year		= {1990}
}

@Article{	  coltheart.avons.ea91,
  author	= {Coltheart, Veronika and Avons, S. E. and Masterson,
		  Jacqueline and Laxon, Veronica J.},
  title		= {The role of assembled phonology in reading comprehension},
  journal	= {Memory and Cognition},
  volume	= {19},
  number	= {4},
  pages		= {387-400},
  abstract	= {Assessed the contribution of assembled phonology to
		  phonological effects in reading comprehension in 4
		  experiments with 121 university students and staff. In Exp
		  1, Ss judged the acceptability of sentences with regular,
		  exception, and nonword homophone substitutions and
		  orthographic controls. Significantly more errors occurred
		  with sentences with regular-word homophones than exception
		  words, and error rates (ERs) for nonword homophones were
		  low and not significant. Exp 2 showed that this was not due
		  to differences in the sentence frames. In Exp 3, Ss judged
		  as unacceptable those sentences containing an exception
		  word that sounded correct when read according to
		  spelling-to-sound rules. Significantly higher ERs occurred
		  only for low- frequency exception words. Exp 4 showed that
		  task conditions affect semantic-categorization ERs for
		  nonword homophones.},
  year		= {1991}
}

@Article{	  coltheart.curtis.ea93,
  author	= {Coltheart, Max and Curtis, Brent and Atkins, Paul and
		  Haller, Micheal},
  title		= {Models of reading aloud: {D}ual-route and
		  parallel-distributed-processing approaches},
  journal	= {Psychological Review},
  volume	= {100},
  number	= {4},
  pages		= {589-608},
  abstract	= {It has often been argued that various facts about skilled
		  reading aloud cannot be explained by any model unless that
		  model possesses a dual-route architecture (lexical and
		  nonlexical routes from print to speech). This broad claim
		  has been challenged by M. S. Seidenberg and J. L.
		  McClelland (1989, 1990). Their model has but a single route
		  from print to speech, yet, they contend, it can account for
		  major facts about reading that have hitherto been claimed
		  to require a dual-route architecture. The authors identify
		  6 of these major facts about reading. The 1-route model
		  proposed by Seidenberg and McClelland can account for the
		  1st of these but not the remaining 5. Because models with
		  dual-route architectures can explain all 6 of these basic
		  facts about reading, the authors suggest that this remains
		  the viable architecture for any tenable model of skilled
		  reading and learning to read. The dual-route cascaded
		  model, a computational version of the dual-route model, is
		  described.},
  year		= {1993}
}

