@Article{	  plaut.farah90,
  author	= {Plaut, David C. and Farah, Martha J.},
  title		= {Visual object representation: Interpreting
		  neurophysiological data within a computational framework},
  journal	= {Journal of Cognitive Neuroscience},
  volume	= {2},
  number	= {4},
  pages		= {320-343},
  abstract	= {Current computational theories of object recognition can
		  provide a much-needed theoretical framework for
		  interpreting the findings of visual neurophysiology, while
		  neurophysiological data can provide important constraints
		  on computational theories of vision. A computational
		  framework for the design of object representations is
		  presented together with computational criteria that can be
		  used to characterize the design space for object
		  representations induced by issues related to design
		  primitives, spatial reference frame, and organization.
		  Implementation issues are addressed; and pertinent data are
		  reviewed, including single- cell recordings and lesion
		  studies in humans and monkeys. Interpretations of
