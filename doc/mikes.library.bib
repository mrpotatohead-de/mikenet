@Article{	  antony91,
  author	= {Antony, Michael V.},
  title		= {Fodor and Pylyshyn on connectionism},
  journal	= {Minds & Machines},
  volume	= {1},
  number	= {3},
  pages		= {321-341},
  abstract	= {Considers (1) J. Fodor and Z. Pylyshyn's (see PA, Vol
		  76:3804) failure to support the premise that the cognitive
		  architecture is a Classical architecture, and (2) their
		  defense of the claim that Classicalism and Connectionism
		  (CAC) are incompatible. Fodor and Pylyshyn's defense of CAC
		  is criticized, and reasons for thinking that the defense
		  might be false are offered. The implications of what the
		  falsity of the defense would imply for the Classicism-
		  Connectionism debate are discussed. It is argued that
		  Connectionist architectures can instantiate Languages of
		  Thought, as Fodor and Pylyshyn defined that notion.},
  year		= {1991}
}

