@Article{	  shallice.kartsounis93,
  author	= {Shallice, T. and Kartsounis, L. D.},
  title		= {Selective impairment of retrieving people's names: A
		  category specific disorder?},
  journal	= {Cortex},
  volume	= {29},
  number	= {2},
  pages		= {281-291},
  abstract	= {Describes the case of a 56-yr-old male patient with an
		  extremely long-standing low grade glioma affecting the left
		  temporal lobe. The patient was almost entirely unable to
		  retrieve the names of current personalities, although in
		  other respects identification was unimpaired and nominal
		  functions were only mildly inefficient. In particular,
		  geographical features and historical figures were generally
		  appropriately named. The problematic retrieval was equally
		  severe whether naming was to confrontation, from
		  description, or by generation. A similarly severe
		  impairment was also found for the retrieval of new words
		  that had come into the language in the last 20 yrs (e.g.,
		  AIDS). The impairment of retrieving people's names was
		  interpreted in terms of a long- standing inability to form
		  new associations between meaning and phonological
		  word-forms.},
  year		= {1993}
}

@Article{	  shallice.mcleod.ea85,
  author	= {Shallice, Tim and McLeod, Peter and Lewis, Kristin},
  title		= {Isolating cognitive modules with the dual-task paradigm:
		  Are speech perception and production separate processes?},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {4},
  pages		= {507-532},
  abstract	= {Used a dual-task paradigm to investigate whether the
		  auditory input logogen, or word-form system, is distinct
		  from the articulatory output logogen in 3 experiments with
		  52 females (aged 20-40 yrs). In Exps I and II it was shown
		  that the task of detecting an unspecified name in an
		  auditory input stream could be combined with reading aloud
		  visually presented words with relatively little single- to
		  dual-task decrement. The stimuli for both tasks were
		  independent streams of random words presented at rapid
		  rates. Exp III suggested that the 1st task places a
		  considerable information processing load on the auditory
		  input logogen, that the 2nd task places a considerable load
		  on the phonological output logogen, and that Ss do not
		  switch between the 2 tasks. That the 2 tasks can be
		  combined with ease supports the view that the systems
		  underlying reading aloud and listening are separate. The
		  ease of performance when the input streams are in different
		  modalities, compared with the difficulties when they are in
		  the same, has implications for general models of
		  attention.},
  year		= {1985}
}

@Article{	  shallice.saffran86,
  author	= {Shallice, Tim and Saffran, Eleanor},
  title		= {Lexical processing in the absence of explicit word
		  identification: Evidence from a letter-by-letter reader},
  journal	= {Cognitive Neuropsychology},
  volume	= {3},
  number	= {4},
  pages		= {429-458},
  abstract	= {The lexical decision performance of a letter-by-letter
		  reader (a 72-yr-old male) was analyzed for words presented
		  for too short an exposure duration for him to identify or
		  name them. His performance was surprisingly good by
		  comparison with that of previously reported patients. The S
		  gave a similar pattern of responses to inappropriately and
		  appropriately affixed words. His performance on these tasks
		  could not be attributed to neglect or to the use of letter
		  sequential dependencies. With the same exposure duration,
		  the S showed semantic access characteristics in word
		  categorization tasks. This evidence for lexical processing
		  in the absence of explicit identification is discussed in
		  relation to alternative accounts of letter-by-letter
		  reading.},
  year		= {1986}
}

@Article{	  shallice.warrington.ea83,
  author	= {Shallice, Tim and Warrington, Elizabeth K. and McCarthy,
		  Rosaleen},
  title		= {Reading without semantics},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {1},
  pages		= {111-138},
  abstract	= {Analysis of the reading by a 66-yr-old female neurological
		  patient, diagnosed as having a progressive dementing
		  illness of unknown etiology, indicated that her reading was
		  based on the operation of a relatively unimpaired
		  phonological route. Quantitative investigations of type of
		  error, reading speed, and the effects of lexicality and
		  spelling-to-sound regularity all supported this conclusion.
		  Spelling-to-sound regularity was shown to influence reading
		  not only through regular words being better read than
		  irregular ones but also through mildly irregular words
		  being better read than very irregular ones. A model is
		  presented of the operation of the phonological route and of
		  its progressive impairment in certain types of neurological
		  disease.},
  year		= {1983}
}

