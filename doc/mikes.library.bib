@Article{	  laxon.coltheart.ea88,
  author	= {Laxon, Veronica J. and Coltheart, Veronika and Keating,
		  Corriene},
  title		= {Children find friendly words friendly too: {Words} with
		  many orthographic neighbours are easier to read and spell},
  journal	= {British Journal of Educational Psychology},
  volume	= {58},
  number	= {1},
  pages		= {103-119},
  abstract	= {49 children (aged 8 yrs 4 mo to 10 yrs 4 mo) were grouped
		  according to reading ability (SPAR Spelling and Reading
		  Tests) and were tested on lexical decision, naming and
		  spelling of words and nonwords that differed in
		  orthographic neighborhood size. Friendly words (with many
		  orthographic neighbors) caused significantly fewer errors
		  on all these tasks. Item analysis demonstrated that these
		  effects were robust when corrected for frequency,
		  regularity, word length, and age of acquisition. Good
		  readers showed less reliance on neighborhood size and more
		  evidence of accurate use of grapheme-phoneme correspondence
		  rules. Results suggest that children find common
		  orthographic sequences easier to read and spell before they
		  have learned to use grapheme-phoneme correspondences
		  consistently.},
  year		= {1988}
}

@Article{	  laxon.masterson.ea91,
  author	= {Laxon, Veronica and Masterson, Jacqueline and Coltheart,
		  Veronika},
  title		= {Some bodies are easier to read: {The} effect of
		  consistency and regularity on children's reading},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {4},
  pages		= {793-824},
  abstract	= {Consistency and regularity were examined as separate
		  factors in children's reading, by devising stimuli in
		  accordance with the different types of 3-letter endings
		  that are proposed within the body subsystem. The idea of
		  the body segment (which consists of the vowel and terminal
		  letters of a monomorphemic, monosyllabic word), as proposed
		  by K. E. Patterson and J. Morton (1985), is to capture
		  consistency effects in reading aloud. In 2 experiments, 87
		  children (aged 7-9 yrs) were subdivided according to
		  reading ability and given words and nonwords to read aloud.
		  In all the Ss, performance was affected by body type for
		  both words and nonwords, but the better readers were most
		  affected.},
  year		= {1991}
}

