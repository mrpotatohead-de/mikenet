@Article{	  stuart.coltheart88,
  author	= {Stuart, Morag and Coltheart, Max},
  title		= {Does reading develop in a sequence of stages?},
  journal	= {Cognition},
  volume	= {30},
  number	= {2},
  pages		= {139-181},
  abstract	= {Discusses 3 recent stage theories of reading acquisition
		  (G. Marsh et al, 1981; U. Frith, 1985; and P. H. Seymour
		  and C. J. MacGregor, (see PA, Vol 72:14866)). The
		  relationship between phonological awareness and reading is
		  outlined. Data are presented from an ongoing longitudinal
		  study of reading acquisition in 23 English-speaking nursery
		  school students (born in 1978) that included assessments of
		  prereading phonological skills. Results suggest that (a)
		  even if learning to read is conceptualized as a sequence of
		  stages, not all children pass through the same sequence of
		  stages; (b) phonological awareness and reading acquisition
		  have a reciprocal interactive causal relationship, not a
		  unidirectional one; and (c) phonological skills can play a
		  role in the 1st stage of learning among phonologically
		  adept children. (French abstract)},
  year		= {1988}
}

