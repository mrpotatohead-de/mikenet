@Article{	  seidenberg.bruck.ea85,
  author	= {Seidenberg, Mark S. and Bruck, Margaret and Fornarolo,
		  Gail and Backman, Joan},
  title		= {Word recognition processes of poor and disabled readers:
		  Do they necessarily differ?},
  journal	= {Applied Psycholinguistics},
  volume	= {6},
  number	= {2},
  pages		= {161-179},
  abstract	= {Tested the hypothesis that children assessed as reading
		  disabled use decoding processes that differ from those of
		  nondisabled children by comparing the word recognition
		  skills of 17 clinic-diagnosed reading disabled (dyslexic)
		  3rd and 4th graders with those of 17 good and 17 poor 3rd
		  and 4th grade readers, as determined by the Reading
		  Comprehension subtest of the Stanford Diagnostic Reading
		  Test. All Ss were administered the Ravens Coloured
		  Progressive Matrices and the Peabody Picture Vocabulary
		  Test (PPVT). Ss read words and nonwords containing either
		  regular (single pronunciation) or homographic (multiple
		  pronunciation) spelling patterns. Analyses of the errors,
		  latencies, and types of pronunciations indicated that while
		  the performance of the poor and disabled readers differed
		  from that of the good readers, the 2 below-average reader
		  groups were very similar. Reading disabled Ss exhibited
		  decoding processes similar to those exhibited by younger
		  nondisabled Ss. Results suggest that many children who meet
		  the diagnostic criteria for reading disability may be
		  indistinguishable from nondisabled children in terms of
		  actual reading performance.},
  year		= {1985}
}

@Article{	  seidenberg.bruck.ea86,
