@Article{	  waters.seidenberg.ea84,
  author	= {Waters, Gloria S. and Seidenberg, Mark S. and Bruck,
		  Margaret},
  title		= {Children's and adults' use of spelling-sound information
		  in three reading tasks},
  journal	= {Memory and Cognition},
  volume	= {12},
  number	= {3},
  pages		= {293-305},
  abstract	= {Examined the effects of irregular spelling and
		  spelling-sound correspondences on word recognition of 49
		  3rd and 5th graders and 45 undergraduates. Results indicate
		  that adults showed effects of spelling and spelling-sound
		  irregularities in reading lower frequency words on all 3
		  tasks (lexical decision, naming, and sentence recognition),
		  whereas younger and poorer readers (reading skill was
		  assessed using the Comprehension subtest of the Stanford
		  Diagnostic Reading Test) also showed effects on higher
		  frequency words. The fact that irregular spelling-sound
		  correspondences affected performance on the sentence task
		  indicated that access of phonological information is not an
		  artifact of having to read a word aloud or perform a
		  lexical decision. Two other developmental trends were
		  observed: As children became more skilled in reading, the
		  effects of irregular spelling were overcome before the
		  effects of irregular spelling-sound correspondences; the
		  latter effects were eliminated on silent reading tasks
		  earlier than on the naming task.},
  year		= {1984}
}

@Article{	  waters.seidenberg85,
  author	= {Waters, Gloria S. and Seidenberg, Mark S.},
  title		= {Spelling-sound effects in reading: Time course and
		  decision criteria},
  journal	= {Memory and Cognition},
  volume	= {13},
  number	= {6},
  pages		= {557-572},
  abstract	= {Examined predictions of the time-course model of word
		  recognition in 6 experiments with 254 undergraduates. The
		  model suggests that effects of irregular spelling or
		  pronunciation should be specific to more slowly recognized
		  words, such as lower frequency items. The model also
		  explains why effects of irregular pronunciation are more
		  robust in naming than in lexical decisions: Only the
		  effects in lexical decisions depend on Ss' response
		  criteria. It was shown that these criteria are affected by
		  the composition of the stimuli in an experiment (i.e.,
		  whether irregularly spelled words are present) and by
		  pressure to respond quickly. In contrast to the dual-route
		  model of word recognition, the time-course model accounted
		  for these task differences without assuming that Ss
		  strategically control access to phonology. Effects of
		  irregular pronunciation were specific to lower frequency
		  words. Median reaction times (RTs) and number of errors by
		  items are appended for the 6 experiments.},
  year		= {1985}
}

