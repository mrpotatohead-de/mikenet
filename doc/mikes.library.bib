@Article{	  mcrae.jared.ea90,
  author	= {McRae, Ken and Jared, Debra and Seidenberg, Mark S.},
  title		= {On the roles of frequency and lexical access in word
		  naming},
  journal	= jml,
  volume	= {29},
  number	= {1},
  pages		= {43-65},
  abstract	= {Investigated the loci of frequency effects in a naming
		  task in Exps 1, 2, and 3 with 39 university students, and
		  in Exp 4 with 12 other students. On immediate naming
		  trials, Ss read words aloud as soon as they were presented.
		  On delayed naming trials, Ss initiated pronunciation after
		  a response signal several hundred milliseconds following
		  stimulus presentation. Stimuli were high and low frequency
		  homophones (e.g., MAIN/MANE), high and low frequency rhymes
		  (e.g., COLD/FOLD), and word/pseudohomophone pairs (e.g.,
		  SCAM/SKAM). The magnitude of frequency effects in naming
		  depended on the amount of time prior to the response
		  signal. Large effects obtained in immediate naming were
		  eliminated at the longest delay intervals. Results are
		  interpreted in terms of current models of word recognition
		  and naming, specifically the role of lexical access and the
		  computational basis of frequency effects.},
  year		= {1990}
}

