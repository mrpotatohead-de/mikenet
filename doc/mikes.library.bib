@Article{	  plaut95,
  author	= {Plaut, David C.},
  title		= {Double dissociation without modularity: Evidence from
		  connectionist neuropsychology. Special Issue: Modularity
		  and the brain},
  journal	= {Journal of Clinical and Experimental Neuropsychology},
  volume	= {17},
  number	= {2},
  pages		= {291-321},
  abstract	= {Reviews empirical data on a double dissociation between
		  concrete and abstract word reading after damage to a
		  connectionist network that pronounces words via meaning and
		  yet has no separable components (D. C. Plaut and T.
		  Shallice; see PA, Vol 81:10068). The functional
		  specialization in the network that gives rise to the double
		  dissociation is not transparently related to the network's
		  structure, as modular theories assume. Consideration of the
		  distribution of effects across quantitatively equivalent
		  individual lesions in the network raises concerns about the
		  interpretation of single-case studies. Findings underscore
		  the necessity of relating neuropsychological data to
		  cognitive theories in the context of computational
		  assumptions about the cognitive system's operation after
		  damage.},
