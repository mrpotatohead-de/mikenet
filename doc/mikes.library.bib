@Article{	  seidenberg93,
  author	= {Seidenberg, Mark S.},
  title		= {Connectionist models and cognitive theory},
  journal	= {Psychological Science},
  volume	= {4},
  number	= {4},
  pages		= {228-235},
  abstract	= {Identifies the bases of disagreements among cognitive
		  psychologists regarding the relationship between theories
		  and computational models and discusses their implications
		  for connectionist modeling. The role of connectionist
		  modeling in explaining behavior in cognitive science and
		  cognitive neuroscience has generated considerable debate.
		  Connectionism can be treated as a form of simulation
		  modeling in which the goal is to implement preexisting
		  theories. In this approach, connectionist models function
		  as a statistical tool, a way of analyzing a complex set of
		  data. Connectionism can also be seen as providing a small
		  set of general theoretical principles that apply in a
		  variety of domains. Construed in this way, it contributes
		  to the development of theories that are explanatory, not
		  merely descriptive.},
  year		= {1993}
}

@Article{	  seidenberg93*1,
  author	= {Seidenberg, Mark S.},
  title		= {A connectionist modeling approach to word recognition and
		  dyslexia. American Psychological Society Meetings (1992,
		  San Diego, California)},
  journal	= {Psychological Science},
  volume	= {4},
  number	= {5},
  pages		= {299-304},
  abstract	= {Develops a theory of word recognition that is realized as
		  a connectionist simulation model. The approach involves 4
		  components: reading theory, connectionist principles,
		  implemented model, and behavioral data. Experience with a
		  first-generation model suggests that the approach can
		  reveal general principles underlying word recognition and
		  its impairments. Although computational modeling introduces
		  new problems of method and interpretation, it contributes
		  in an essential way to understanding reading and other
		  aspects of cognition.},
  year		= {1993}
}

@Article{	  seidenberg94,
  author	= {Seidenberg, Mark S.},
  title		= {Language and connectionism: {T}he developing interface},
  journal	= {Cognition},
  volume	= {50},
  number	= {1-3},
  pages		= {385-401},
  abstract	= {Suggests that connectionist concepts are now beginning to
		  be brought to bear on basic issues concerning the
		  structure, acquisition, and processing of language, both
		  normal and disordered. Potential points of further contact
		  between connectionism and linguistic theory are described.
		  The author considers how connectionist concepts may be
		  relevant to issues concerning (1) the representation of
		  linguistic knowledge, (2) the role of a priori constraints
		  on acquisition, and (3) the poverty of the stimulus
		  argument. These models may contribute to the development of
		  explanatory theories of language.},
  year		= {1994}
}

