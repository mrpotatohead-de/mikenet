@Article{	  backman.bruck.ea84,
  author	= {Backman, Joan and Bruck, Margaret and Hebert, Martine and
		  Seidenberg, Mark S.},
  title		= {Acquisition and use of spelling-sound correspondences in
		  reading},
  journal	= jecp,
  volume	= {38},
  number	= {1},
  pages		= {114-133},
  abstract	= {Evaluated the acquisition and use of knowledge concerning
		  the spelling-sound correspondences of English by having
		  children read words and nonwords that contained regular and
		  homographic spelling patterns. 16 good readers each from
		  Grades 2, 3, and 4 and 16 poor readers each from Grades 3
		  and 4 served as Ss. Regular spelling patterns were
		  associated with a single pronunciation, and homographic
		  patterns had multiple pronunciations. Analyses of errors,
		  latencies, and pronunciations provided evidence for 2
		  complementary developmental processes: Good beginning
		  readers rapidly learned to recognize high-frequency words
		  from visual input alone, while at the same time they
		  expanded and consolidated their knowledge of spelling-sound
		  correspondences. Younger and poor readers relied more on
		  phonological information in word decoding, as evidenced by
		  their particular difficulty reading homographic spelling
		  patterns. Poor readers did not appear to use a radically
		  different strategy for reading words: Their performance was
		  similar to that of younger, good readers.},
  year		= {1984}
}

