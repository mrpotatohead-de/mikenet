@Article{	  kawamoto93,
  author	= {Kawamoto, Alan H.},
  title		= {Time-course of processing in feed-forward connectionist
		  networks: {A}n analysis of the exclusive-or problem},
  journal	= {Journal of Mathematical Psychology},
  volume	= {37},
  number	= {4},
  pages		= {556-574},
  abstract	= {In this Monte Carlo simulation study, 2 functional classes
		  of solutions of the exclusive-or problem that arise in a
		  feed- forward network (FFN) comprised of 2 input units, 2
		  hidden units, and 1 output unit (OU) are described. The 2
		  classes are distinguished by the time-course of activation
		  (TCA) of the OU in response to 1 particular input. For 1
		  class, the activation of the OU is virtually unchanged over
		  time. For the other, the activation increases and then
		  decreases. After examining the basis for this distinction,
		  networks with different architectures are considered to
		  determine the effect of increasing and decreasing the
		  degrees of freedom. The implications of a TCA that changes
		  nonmonotonically in FFNs with and without lateral
		  inhibitory connections for simulating fast errors in
		  choice-response time experiments are examined.},
  year		= {1993}
}

@Article{	  kawamoto93*1,
  author	= {Kawamoto, Alan H.},
  title		= {Nonlinear dynamics in the resolution of lexical ambiguity:
		  {A} parallel distributed processing account},
  journal	= jml,
  volume	= {32},
  number	= {4},
  pages		= {474-516},
  abstract	= {Presents a connectionist account of lexical ambiguity
		  resolution in which the nonlinear dynamics that arise
		  during processing are emphasized. In the implementation,
		  the spelling, pronunciation, part of speech, and meaning of
		  both ambiguous and unambiguous words are represented as a
		  distributed pattern of activity over a set of simple
		  processing units in a fully recurrent network. After the
		  network is trained on this lexicon using an
		  error-correction algorithm, the performance of the network
		  is assessed by presenting just part of a lexical entry
		  (e.g., the spelling). The simulations demonstrate the
		  effect of frequency and context on the processing of
		  unambiguous words, and the effect of relative dominance and
		  context on the time course of the activation of meanings of
		  ambiguous words.},
  year		= {1993}
}

