@Article{	  geman.bienenstock.ea92,
  author	= {Geman, Stuart and Bienenstock, Elie and Doursat, Rene},
  title		= {Neural networks and the bias/variance dilemma},
  journal	= {Neural Computation},
  volume	= {4},
  number	= {1},
  pages		= {1-58},
  abstract	= {Feedforward neural networks trained by error
		  backpropagation are examples of nonparametric regression
		  estimators. A tutorial is presented on nonparametric
		  inference and its relation to neural networks, and the
		  statistical viewpoint is used to highlight strengths and
		  weaknesses of neural models. The main points are
		  illustrated with some recognition experiments involving
		  artificial data as well as handwritten numerals. In
		  conclusion, it is suggested that current- generation
		  feedforward neural networks are inadequate for difficult
		  problems in machine perception and machine learning,
		  regardless of parallel-vs-serial hardware or other
		  implementation issues. The fundamental challenges in neural
		  modeling are about representation rather than learning.
		  This last point is supported by additional experiments with
		  handwritten numerals.},
  year		= {1992}
}

