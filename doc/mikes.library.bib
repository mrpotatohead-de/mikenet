@Article{	  coltheart.laxon.ea86,
  author	= {Coltheart, Veronika and Laxon, Veronica J. and Keating, G.
		  Corriene and Pool, Margaret M.},
  title		= {Direct access and phonological encoding processes in
		  children's reading: {E}ffects of word characteristics},
  journal	= {British Journal of Educational Psychology},
  volume	= {56},
  number	= {3},
  pages		= {255-270},
  abstract	= {Studied the development of direct access and phonological
		  encoding process in young readers. In Exp I, approximately
		  45 children (aged 8 yrs 10 mo to 9 yrs 7 mo) completed 5
		  reading tasks in which psycholinguistic properties of words
		  were manipulated. Word imaginability affected reading
		  accuracy. Accuracy in reading aloud and judgments of
		  phonology were lower for nonwords than for words. Only the
		  best readers had well-developed skills in
		  grapheme-to-phoneme sound conversion, and all appeared to
		  rely on direct visual access when reading. In Exp II, using
		  the same Ss from Exp I, a sentence comprehension task
		  investigated the role of phonological coding in reading for
		  meaning. Phonological effects were evident since Ss found
		  it difficult to reject meaningless sentences that sounded
		  correct.},
  year		= {1986}
}

@Article{	  coltheart.laxon.ea88,
  author	= {Coltheart, Veronika and Laxon, Veronica J. and Keating,
		  Corriene},
  title		= {Effects of word imageability and age of acquisition on
		  children's reading},
  journal	= {British Journal of Psychology},
  volume	= {79},
  number	= {1},
  pages		= {1-12},
  abstract	= {In Exp I, 47 9-yr-old children had to read aloud words
		  differing in age of acquisition (but matched for
		  imageability, length, and frequency). A highly significant
		  effect of age of acquisition on reading accuracy was
		  obtained. Ss also had to read aloud words differing in
		  imageability (but matched for age of acquisition, length,
		  and frequency). Although imageability affected accuracy,
		  the difference in high- and low-imageability words was
		  significant only for the poorer reader. In Exp II, which
		  utilized 20 undergraduates, adult naming latencies for the
		  same tasks were significantly lower for words acquired
		  early than for words acquired later. Word imageability did
		  not affect adult naming latencies.},
  year		= {1988}
}

@Article{	  coltheart.laxon.ea88*1,
  author	= {Coltheart, Veronika and Laxon, Veronica and Rickard, Mary
		  and Elton, Caroline},
  title		= {Phonological recoding in reading for meaning by adults and
		  children},
  journal	= jepmlc,
  volume	= {14},
  number	= {3},
  pages		= {387-397},
  abstract	= {Phonological recoding in reading has been studied by
		  requiring adults or children to judge whether printed
		  sentences are correct or not. When some sentences are
		  orthographically unacceptable but phonologically acceptable
		  (e.g., The girl through the ball), both children and adults
		  make abnormally many false positives with such sentences.
		  It is unclear whether the phonological recoding that
		  produces this effect is attributable to assembled
		  (nonlexical) or addressed (lexical) phonology. Two types of
		  phonologically acceptable but orthographically unacceptable
		  sentences were devised: Those in which the crucial item
		  ("through" in the above example) was an irregular word (so
		  that its phonology could only be obtained lexically), and
		  those in which the crucial item was a homophonic nonword
		  (so that its phonology could only be obtained by assembled
		  phonology). Both types of sentence produced significantly
		  high false-positive rates for adult readers and children,
		  indicating the use of assembled and addressed phonology
		  during sentence reading.},
  year		= {1988}
}

@Article{	  coltheart.laxon90,
  author	= {Coltheart, Veronika and Laxon, Veronica J.},
  title		= {The development of phonological mediation in reading
		  comprehension},
  journal	= {Language and Cognitive Processes},
  volume	= {5},
  number	= {2},
  pages		= {81-104},
  abstract	= {Four experiments, using 106 2nd-5th graders, investigated
		  (1) the importance of "addressed" (ADP) and "assembled"
		  phonology (ASP) for phonological encoding in children who
		  were learning to read and (2) how the relative importance
		  of these 2 procedures might change over the age range 6-8
		  yrs. Both forms of phonological recoding (PR) occurred with
		  the older Ss. With younger Ss, PR due to ADP was observed.
		  In schools which emphasized phonics, teaching younger Ss
		  also showed an effect due to ASP. Teaching methods
		  influenced the acquisition of these 2 forms of PR. However,
		  even in the absence of phonics teaching, skills in ASP were
		  acquired as Ss learned to read, so that effects due to ASP
		  were evident at 7 yrs of age even in children who have had
		  no formal phonics instruction.},
  year		= {1990}
}

@Article{	  coltheart.patterson.ea94,
  author	= {Coltheart, Veronika and Patterson, Karalyn and Leahy,
		  Judi},
  title		= {When a ROWS is a ROSE: {P}honological effects in written
		  word comprehension},
  journal	= {Quarterly Journal of Experimental Psychology: Human
		  Experimental Psychology},
  volume	= {4},
  pages		= {917-955},
  abstract	= {Conducted 5 experiments with a total of 131 undergraduates
		  and found that when skilled readers make speeded
		  categorization judgments about printed words, errors occur
		  to homophones (HOMs) of real category exemplars. In Exps 1
		  and 2, Ss incorrectly accepted both the word STEAL (as a
		  member of the category A METAL) and the nonword JEAP (as A
		  VEHICLE) significantly more often than incorrect
		  non-homophonic items. Exp 3 demonstrated equivalent error
		  rates for HOM targets differing from real exemplars by
		  various types of single- letter change, but reduced error
		  rates, especially for non- word HOMs, when Ss were
		  instructed to accept only correctly spelled instances. Exps
		  4 and 5 established that the magnitude of the HOM effect is
		  predicted by the degree of orthographic similarity between
		  homophonic mates but not by spelling-sound regularity of
		  the presented HOMs. Results suggest that automatic
		  phonological activation plays a major role in the
		  comprehension of written words.},
  year		= {1994}
}

