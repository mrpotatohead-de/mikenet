@Article{	  seidenberg.waters.ea84,
  author	= {Seidenberg, Mark S. and Waters, Gloria S. and Sanders,
		  Michael and Langer, Pearl},
  title		= {Pre- and postlexical loci of contextual effects on word
		  recognition},
  journal	= {Memory and Cognition},
  volume	= {12},
  number	= {4},
  pages		= {315-328},
  abstract	= {The context in which a word occurs could influence either
		  the actual decoding of the word or a postrecognition
		  judgment of the relatedness of word and context. The
		  present 4 experiments, with 224 undergraduates,
		  investigated the loci of contextual effects that occur in
		  lexical priming, when prime and target words are related
		  along different dimensions. Both lexical decision and
		  naming tasks were used because previous research had
		  suggested that they are differentially sensitive to
		  postlexical processing. Semantic and associative priming
		  occurred with both tasks. Other facilitative contextual
		  effects, due to syntactic relations between words, backward
		  associations, or changes in the proportion of related
		  items, occurred only with the lexical decision task.
		  Results indicate that only associative and semantic priming
		  facilitate the decoding of a target; the other effects are
		  postlexical. Findings are related to the different demands
		  of the naming and lexical decision tasks and to current
		  models of word recognition.},
  year		= {1984}
}

@Article{	  seidenberg.waters.ea84*1,
  author	= {Seidenberg, Mark S. and Waters, Gloria S. and Barnes,
		  Marcia A. and Tanenhaus, Michael K.},
  title		= {When does irregular spelling or pronunciation influence
		  word recognition?},
  journal	= {Journal of Verbal Learning and Verbal Behavior},
  volume	= {23},
  number	= {3},
  pages		= {383-404},
  abstract	= {Conducted 4 experiments with 132 undergraduates to examine
		  how irregularities in spelling or in the correspondence
		  between spelling and sound influence 2 reading tasks,
		  naming and lexical decision. The words studied included
		  regular (e.g., must ), exception ( have ), regular
		  inconsistent ( gave ) words and words with unusual spelling
		  patterns such as aisle or fuel. Results indicate that the 2
		  factors have separate effects on recognition. Irregular
		  spelling influenced performance on both tasks, while
		  irregular spelling-sound correspondences only influenced
		  reading aloud. However, all of these effects were
		  restricted to lower frequency words. The processing of both
		  high and low frequency words, as well as the task
		  differences, can be accommodated by a model that considers
		  the time course of the activation of orthographic and
		  phonological information.},
  year		= {1984}
}

