@Article{	  murphy.pollatsek.ea88,
  author	= {Murphy, Lorna A. and Pollatsek, Alexander and Well, Arnold
		  D.},
  title		= {Developmental dyslexia and word retrieval deficits},
  journal	= {Brain and Language},
  volume	= {35},
  number	= {1},
  pages		= {1-23},
  abstract	= {14 10-11 yr old developmental dyslexics, selected on the
		  basis of very slow naming rates on rapid automatic naming
		  tasks, were compared with 14 age- and IQ-matched normal
		  readers on oral language, picture categorization, and
		  reading tasks. Findings indicate that the dyslexics' word
		  retrieval deficits were 1 symptom of a more generalized
		  oral language deficit that involved both receptive and
		  expressive oral language functioning. There were no
		  significant differences between dyslexics and controls on a
		  nonverbal semantic memory task (picture categorization). On
		  a lexical decision task, the dyslexics were slower in
		  making negative decisions for pseudohomophones (e.g.,
		  braik) than for other matched nonwords.},
  year		= {1988}
}

@Article{	  murphy.pollatsek94,
  author	= {Murphy, Lorna and Pollatsek, Alexander},
  title		= {Developmental dyslexia: {H}eterogeneity without discrete
		  subgroups},
  journal	= {Annals of Dyslexia},
  volume	= {44},
  pages		= {120-146},
  abstract	= {Investigated individual differences among 65 10- to 13-yr-
		  olds with dyslexia in reading, phonemic segmentation, and
		  word retrieval. Performance was compared with 65 reading-
		  level-matched and 17 age-matched controls. Global reading
		  scores were obtained from the Wide Range Achievement Test
		  and the Gray Oral Reading Test. Ss also completed 10 other
		  measures of reading ability, as well as 5 oral language
		  tests. Results showed similar variability in performance on
		  reading tasks for the dyslexic and reading-age control
		  groups. The reading-age controls were more proficient on
		  all reading measures relying on assembled phonology and
		  showed a significant degree of accuracy on the oral
		  language task of phoneme segmentation. Chronological-age
		  controls were more proficient on almost all reading
		  measures.},
  year		= {1994}
}

