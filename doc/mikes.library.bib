@Article{	  taraban.mcclelland87,
  author	= {Taraban, Roman and McClelland, James L.},
  title		= {Conspiracy effects in word pronunciation},
  journal	= {Journal of Memory \& Language},
  volume	= {26},
  number	= {6},
  pages		= {608-631},
  abstract	= {Investigated the effect of orthographically similar words
		  (neighbors) on the pronunciation of a target word, using
		  122 undergraduates. In Exp I, it was found that
		  pronunciation latencies for words whose pronunciations were
		  inconsistent with most of their neighbors were slow
		  compared with consistent controls. In Exps II and III,
		  trials on target words or nonwords were preceded by priming
		  with specific neighbors of the target. Exp II showed that
		  preceding a target word with an exceptional neighbor
		  produced an effect on the accuracy and latency of
		  pronunciation of the target. Exp III amplified the effect
		  found in Exp II by using pseudoword targets. Findings are
		  consistent with predictions of conspiracy models.},
  year		= {1987}
}

