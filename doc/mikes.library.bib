@Article{	  seidenberg85cog,
  author	= {Seidenberg, Mark S.},
  title		= {The time course of phonological code activation in two
		  writing systems},
  journal	= {Cognition},
  volume	= {19},
  number	= {1},
  pages		= {1-30},
  abstract	= {Conducted 2 studies of word recognition in 2 writing
		  systems- -English and Chinese--that are structured along
		  different principles (alphabetic and logographic,
		  respectively). In Exp I, 22 native speakers of Cantonese
		  were presented with 2 sets of stimuli containing 4 types of
		  characters, a result of crossing the factors type
		  (phonogram, nonphonogram) and frequency (high, low). The
		  phonograms are compound characters containing phonetics
		  that provide information relevant to pronunciation. In Exp
		  II, 48 undergraduate native speakers of English were also
		  presented with 2 sets of stimuli but consisting of words
		  with regular and irregular sound-spelling correspondences
		  of high and low frequency. Results indicate that in each
		  writing system, a large pool of higher frequency words is
		  recognized on a visual basis, without phonological
		  mediation. Phonology only enters into the processing of
		  lower frequency words. Findings are consistent with a
		  parallel interactive model of word recognition in which
		  orthographic and phonological information are activated at
		  different latencies. (French abstract)},
  year		= {1985}
}

