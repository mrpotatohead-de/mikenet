@Article{	  forster94,
  author	= {Forster, Kenneth I.},
  title		= {Computational modeling and elementary process analysis in
		  visual word recognition. {S}pecial Section: {M}odeling
		  visual word recognition},
  journal	= jephpp,
  volume	= {20},
  number	= {6},
  pages		= {1292-1310},
  abstract	= {An attempt is made to isolate the assumptions that make a
		  connectionist approach to visual word recognition
		  distinctive. These include the commitment to distributed
		  representations, the claim that there is no distinction
		  between lexical and nonlexical systems in the naming task,
		  and the claim that it is possible to map from orthography
		  to meaning without using localized representations. It is
		  argued that merely demonstrating that a network model can
		  perform these tasks is not sufficient and that a detailed
		  theory of how the network performs its tasks must accompany
		  the simulation, because a simulation is not equivalent to
		  an explanation. It is argued that further progress requires
		  detailed modeling and experimental study of the elementary
		  processes assumed to be involved in networks and that it is
		  premature to dismiss alternative models of lexical access
		  such as serial search models.},
  year		= {1994}
}

@Article{	  franklin.howard.ea94,
  author	= {Franklin, Sue and Howard, David and Patterson, Karalyn},
  title		= {Abstract word meaning deafness},
  journal	= {Cognitive Neuropsychology},
  volume	= {11},
  number	= {1},
  pages		= {1-34},
  abstract	= {Reports the case of a male patient who shows impaired
		  auditory comprehension of abstract but not concrete words,
		  in the context of preserved comprehension of all types of
		  written words following a stroke. Good performance on
		  auditory lexical decision for abstract words indicates that
		  the patient is able to access these lexical entries. This
		  pattern may be termed "abstract word meaning deafness." The
		  patient is unable to repeat nonwords or write them to
		  dictation and makes semantic errors (SEs) in real word
		  repetition. Exactly the same pattern of SEs and a
		  particular difficulty with abstract words is found in tasks
		  where the patient is required to write to dictation. His
		  auditory comprehension and repetition impairments are not
		  affected by word frequency. Performance on specific words
		  is inconsistent and is improved following a written cue.},
  year		= {1994}
}

